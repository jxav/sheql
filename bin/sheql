#!/usr/bin/env coffee

sh = require('../src/executor')()
str = process.argv[2]
startYear = process.argv[3] or new Date().getFullYear()
endYear = process.argv[4] or new Date().getFullYear()
dateCollection = sh.executor str, new Date(startYear, 0, 1), new Date(endYear, 11, 31)


for i in dateCollection
    console.log dateCollection[i].toDateString()
